    dim A = 1, B isEmpty = T -> eccezione
    A isEmpty = T , B = 1 -> eccezione
    subConsistent = F -> eccezione
    subConsistent = T -> differenza A,B 

dim A > 1, B qualsiasi:
    impossibile
dim A qualsiasi , B > 1:
    impossibile
    

